{
  "name": "@aboudard/ng-lib",
  "version": "0.0.3",
  "description": "Angular schematics for library",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/aboudard/ng-library-schematics.git"
  },
  "bugs": {
    "url": "https://github.com/aboudard/ng-library-schematics/issues"
  },
  "homepage": "https://github.com/aboudard/ng-library-schematics",
  "schematics": "./schematics/collection.json",
  "scripts": {
    "build:sass": "sass src/styles:src/styles",
    "build": "tsc -p tsconfig.schematics.json",
    "postbuild": "copyfiles src/styles/*.css schematics/collection.json ../../../dist/aboudard/ng-lib/"
  },
  "keywords": [
    "schematics",
    "angular",
    "library"
  ],
  "author": "aboudard",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "peerDependencies": {
    "@angular/common": "^13.2.0",
    "@angular/core": "^13.2.0"
  },
  "dependencies": {
    "tslib": "^2.3.0"
  },
  "ng-add": {
    "save": "dependencies"
  },
  "devDependencies": {
    "copyfiles": "file:../../node_modules/copyfiles",
    "typescript": "file:../../node_modules/typescript"
  }
}
